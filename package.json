    "react-dom": "19.0.0",
    "usehooks-ts": "3.1.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@svgr/webpack": "8.1.0",
    "@types/node": "^20",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "camelcase": "8.0.0",
    "chalk": "5.4.1",
    "cross-env": "7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^9.1.0",
